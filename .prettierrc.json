{
    "tabWidth": 4,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "none",
    "printWidth": 120,
    "svelteSortOrder": "options-styles-scripts-markup",
    "svelteStrictMode": true,
    "svelteAllowShorthand": false,
    "svelteIndentScriptAndStyle": false,
    "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-svelte"]
}
